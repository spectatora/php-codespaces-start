{
  "name": "demo/php-codespaces-starter",
  "description": "Zero-to-PHP template for GitHub Codespaces with PHPUnit",
  "type": "project",
  "require": {},
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors=always",
    "serve": "php -S 0.0.0.0:8000 -t public"
  },
  "minimum-stability": "stable",
  "license": "MIT"
}